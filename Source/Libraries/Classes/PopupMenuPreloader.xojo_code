#tag Class
Protected Class PopupMenuPreloader
Inherits DesktopPopupMenu
	#tag Event
		Sub Opening()
		  RaiseEvent Open
		  Opening = False
		End Sub
	#tag EndEvent

	#tag Event
		Sub SelectionChanged(item As DesktopMenuItem)
		  if NOT Opening then
		    RaiseEvent Change
		  end if
		End Sub
	#tag EndEvent


	#tag Hook, Flags = &h0
		Event Change()
	#tag EndHook

	#tag Hook, Flags = &h0
		Event Open()
	#tag EndHook


	#tag Property, Flags = &h0
		Opening As Boolean = True
	#tag EndProperty


